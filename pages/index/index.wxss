/**index.wxss**/
.search{
  background: #fff;
  height: 60rpx;
  line-height: 60rpx;
}
.searchInput{
  padding:20rpx;
  background: #ff2c4a;
  border: none;
  text-align: center;
  font-size: 14px;
  color:#999;
}
.slide-image {
  width:750rpx;
  height:340rpx;
}
.category{
  display: flex;
  justify-content: space-around;
}
.category image{
  width:128rpx;
  height:140rpx;
}
.floor_title image {
  display: block;
  margin-top:20rpx;
  width:100%;
  height:60rpx;
}
.product_list {
  padding:10rpx;
  display: flex;
  justify-content: space-between;
}
.product_list_left image{
  padding-right: 10rpx;
  width: 232rpx;
  height: 386rpx;
}
product_list_right{
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
}
.product_list_right image {
    width: 233rpx;
    height: 188rpx;
}
